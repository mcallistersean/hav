{
  "presets": [
    "@babel/preset-react",
    [
      "@babel/preset-env",
      {
        "useBuiltIns": "usage", // or "entry"
        "corejs": 3
      }
    ]
  ],
  "plugins": ["@babel/plugin-proposal-class-properties"],
  "env": {
    "hmr": {
      "plugins": ["react-hot-loader/babel"]
    },
    "test": {
      "presets": ["@babel/preset-react", "@babel/preset-env"]
    }
  }
}
